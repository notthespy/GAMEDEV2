            <string>if(Cutscene_Time==0){
    if(Is_Pressed=0){
        if(Menu_Depth==0){
            if(Select_Position==0){
                Menu_Depth+=1
                Select_Position=0
                Menu_Instance[1]="ATTACK"
                exit
            }
            if(Select_Position==2){
                allow_runaway=1
                alarm[0]=30
                Cutscene_Time=1
                exit
            }
        }
        if(Menu_Depth==1 and Menu_Instance[1]=="ATTACK"){
            Menu_Depth=2
            Attack_Num=Select_Position
            Select_Position=0
            exit
        }
        if(Menu_Depth==2){
            if(player.Damage_Modifier!=1){
                minarr[Select_Position].Current_HP-=player.Attacks[Select_Position].Damage*player.Damage_Modifier
                player.Damage_Modifier=1
            }else{
                minarr[0].Current_HP-=player.Attacks[Select_Position].Damage
            }
            if(minarr[0].Current_HP&lt;0){
                minarr[0].Current_HP=0
            }
            Window_Text="Used "+player.Attacks[Select_Position].Name+" On "+player.Attacks[Select_Position].Target
            if(player.Attacks[Select_Position].Effect="DD"){
                player.Damage_Modifier=2
            }
            if(player.Attacks[Select_Position].Effect="P"){
                player.Protected=1
            }
            if(player.Attacks[Select_Position].Effect="PA"){
                minarr[0].Can_Attack=0
            }
            alarm[1]=30
            Cutscene_Time=1
            Select_Position=0
            Menu_Depth=0
            exit
        }
        Is_Pressed=1
    }
}
