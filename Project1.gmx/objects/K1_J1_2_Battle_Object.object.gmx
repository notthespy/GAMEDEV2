        <arguments>
          <argument>
            <kind>1</kind>
            <string>if(Cutscene_Time==0){
    if(Is_Pressed=0){
        if(Menu_Depth==0){
            if(Select_Position==0){
                Menu_Depth+=1
                Select_Position=0
                Menu_Instance[1]="ATTACK"
                exit
            }
            if(Select_Position==2){
                allow_runaway=1
                alarm[0]=30
                Cutscene_Time=1
                exit
            }
        }
        if(Menu_Depth==1 and Menu_Instance[1]=="ATTACK" and playerarr[Character_Turn].Attacks[Select_Position].Effect=="H"){
            playerarr[Character_Turn].Current_HP+=playerarr[Character_Turn].Attacks[Select_Position].Damage
            if(playerarr[Character_Turn].Current_HP&gt;playerarr[Character_Turn].Max_HP){
                playerarr[Character_Turn].Current_HP=playerarr[Character_Turn].Max_HP
            }
            Window_Text="Used "+playerarr[Character_Turn].Attacks[Select_Position].Name+" On Self for "+string(playerarr[Character_Turn].Attacks[Select_Position].Damage)
            if(Character_Turn==Num_Characters){
                Cutscene_Time=1
                Select_Position=0
                Menu_Depth=0
                Player_Turn=0
                alarm[1]=30
                exit
            }else{
                Cutscene_Time=1
                Character_Turn+=1
                Select_Position=0
                Menu_Depth=0
                alarm[1]=30
                exit
            }  
        }
        if(Menu_Depth==1 and Menu_Instance[1]=="ATTACK" and playerarr[Character_Turn].Attacks[Select_Position].Effect=="ALL"){
            Attack_Num=Select_Position
            Select_Position=0
            i=0
            while(i&lt;=Enemy_Number){
                if(playerarr[Character_Turn].Damage_Modifier!=1){
                    minarr[i].Current_HP-=playerarr[Character_Turn].Attacks[Attack_Num].Damage*playerarr[Character_Turn].Damage_Modifier
                }else{
                    minarr[i].Current_HP-=playerarr[Character_Turn].Attacks[Attack_Num].Damage
                }
                i++
            }
            Window_Text="Used "+playerarr[Character_Turn].Attacks[Attack_Num].Name+" On All "
            if(Character_Turn==Num_Characters){
                Cutscene_Time=1
                Select_Position=0
                Menu_Depth=0
                Player_Turn=0
                alarm[1]=30
                exit
            }else{
                Cutscene_Time=1
                Character_Turn+=1
                Select_Position=0
                Menu_Depth=0
                alarm[1]=30
                exit
            }
        }
        if(Menu_Depth==2){
            if(playerarr[Character_Turn].Damage_Modifier!=1){
                minarr[Select_Position].Current_HP-=playerarr[Character_Turn].Attacks[Attack_Num].Damage*playerarr[Character_Turn].Damage_Modifier
                playerarr[Character_Turn].Damage_Modifier=1
            }else{
                minarr[Select_Position].Current_HP-=playerarr[Character_Turn].Attacks[Attack_Num].Damage
            }
            if(minarr[Select_Position].Current_HP&lt;0){
                minarr[Select_Position].Current_HP=0
            }
            Window_Text="Used "+playerarr[Character_Turn].Attacks[Attack_Num].Name+" On Minion "+string(Select_Position)
            if(playerarr[Character_Turn].Attacks[Attack_Num].Effect="DD"){
                playerarr[Character_Turn].Damage_Modifier=2
            }
            if(playerarr[Character_Turn].Attacks[Attack_Num].Effect="P"){
                playerarr[Character_Turn].Protected=1
            }
            if(playerarr[Character_Turn].Attacks[Attack_Num].Effect="PA"){
                minarr[Select_Position].Can_Attack=0
            }
            if(playerarr[Character_Turn].Attacks[Attack_Num].Effect="R"){
                playerarr[Character_Turn].Retaliate=1
            }
            if(Character_Turn==Num_Characters){
                Cutscene_Time=1
                Select_Position=0
                Menu_Depth=0
                Player_Turn=0
                alarm[1]=30
                exit
            }else{
                Cutscene_Time=1
                Character_Turn+=1
                Select_Position=0
                Menu_Depth=0
                alarm[1]=30
                exit
            }
        }
        if(Menu_Depth==1 and Menu_Instance[1]=="ATTACK"){
            Menu_Depth=2
            Attack_Num=Select_Position
            if(minarr[0].Current_HP&gt;0){
                Select_Position=0
            }else{
                if(minarr[1].Current_HP&gt;0){
                    Select_Position=1
                }else{
                    Select_Position=2
                }
            }
            exit
        }
        Is_Pressed=1
    }
}
